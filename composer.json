{
    "name": "harikt/web.expressive",
    "description": "The integration package with DMS and zend expressive",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Elliot Levin",
            "email": "elliotlevin@hotmail.com"
        },
        {
            "name": "Hari KT",
            "email": "kthari85@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dms\\Web\\Expressive\\": "./src/"
        },
        "files" : [
            "./src/helper.php"
        ]
    },
    "require": {
        "php": ">=7.0.0",
        "akrabat/ip-address-middleware": "^1.0",
        "aura/session": "^2.1",
        "beheh/flaps": "^0.2.0",
        "cache/cache": "^0.1.0",
        "dms-org/common.structure": "^0.8",
        "dms-org/core": "^0.8",
        "dms-org/library.metadata": "^0.8.0",
        "harikt/blade-renderer": "^0.4.0",
        "harikt/dms-illuminate-container": "^0.0",
        "illuminate/config": "5.5.*",
        "psr/http-server-middleware": "^1.0",
        "symfony/translation": "3.*",
        "symfony/validator": "3.*",
        "zendframework/zend-diactoros": "^1.7.1",
        "zendframework/zend-expressive": "^3.0.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Dms\\Web\\Expressive\\Tests\\": "./tests/Tests/",
            "Dms\\Core\\Tests\\": "./vendor/dms-org/core/tests/Tests/"
        }
    },
    "require-dev": {
        "dms-org/package.content": "^0.8",
        "dms-org/library.testing": "dev-master",
        "dms-org/common.testing": "dev-master"
    }
}
